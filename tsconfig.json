{
  "compilerOptions": {
    "types": ["cypress", "node"],
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "**/*.tsx",
    "next-env.d.ts",
    "cypress/support/commands.ts",
    ".next/types/**/*.ts",
    "src/app/lo",
    "cypress/support/e2e.tsx",
    "cypress/e2e/page.cy.tsx",
    "cypress/support/commands.tsx",
    "src",
    "cypress"
  ],
  "exclude": ["node_modules"]
}
